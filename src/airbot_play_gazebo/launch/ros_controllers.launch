<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find airbot_play_gazebo)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="airbot_play_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="arm_position_controller gripper_position_controller "/>

</launch>
